<UserControl x:Class="GitFlowVS.Extension.GitFlowSectionUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button x:Name="Init" Content="Init" HorizontalAlignment="Left" Margin="94,10,0,0" VerticalAlignment="Top" Width="90" Click="Button_Click"/>
        <Button x:Name="StartFeature" Content="Start Feature" HorizontalAlignment="Left" Margin="94,35,0,0" VerticalAlignment="Top" Width="90" Click="StartFeature_Click"/>
        <Button x:Name="FinishFeature" Content="Finish Feature" HorizontalAlignment="Left" Margin="94,60,0,0" VerticalAlignment="Top" Width="90" Click="FinishFeature_Click"/>
        <TextBox x:Name="FeatureName" HorizontalAlignment="Left" Height="23" Margin="198,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="92"/>

    </Grid>
</UserControl>
